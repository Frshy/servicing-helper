generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Sale {
  id        Int      @id @default(autoincrement())
  service   String
  price     Float
  orderedBy Int
  editedAt  DateTime @updatedAt
  createdAt DateTime @default(now())
}